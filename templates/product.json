rue\">\n      <span class=\"coin c1\">Â£</span>\n      <span class=\"coin c2\">â‚¬</span>\n      <span class=\"coin c3\">$</span>\n      <span class=\"coin c4\">Â£</span>\n      <span class=\"coin c5\">â‚¬</span>\n      <span class=\"coin c6\">$</span>\n    </span>\n  </div>\n</div>\n\n{% endunless %}"
